<launch>
    <!-- Launch Turtlesim -->
    <node pkg="turtlesim" exec="turtlesim_node" name="turtlesim_node" output="screen"/>

    <!-- Launch Turtle Robot -->
    <node pkg="turtle_brick" exec="turtle_robot" name="turtle_robot" output="screen">
        <param name="frequency" value="10.0"/>
    </node>
</launch>
